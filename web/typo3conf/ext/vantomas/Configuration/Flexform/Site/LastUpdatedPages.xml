<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:vantomas/Resources/Private/Language/Site/LastUpdatedPages.xlf:flexform.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.pageType>
						<TCEforms>
							<label>LLL:EXT:vantomas/Resources/Private/Language/Site/LastUpdatedPages.xlf:flexform.settings.pageType</label>
							<config>
								<type>select</type>
								<default>1</default>
								<special>pagetypes</special>
								<noIconsBelowSelect>TRUE</noIconsBelowSelect>
							</config>
						</TCEforms>
					</settings.pageType>
					<settings.offset>
						<TCEforms>
							<label>LLL:EXT:vantomas/Resources/Private/Language/Site/LastUpdatedPages.xlf:flexform.settings.offset</label>
							<config>
								<type>input</type>
								<size>5</size>
								<eval>trim,int</eval>
								<range>
									<lower>1</lower>
									<upper>10</upper>
								</range>
								<default>1</default>
								<wizards>
									<settings.offset>
										<type>slider</type>
										<step>1</step>
										<width>100</width>
									</settings.offset>
								</wizards>
							</config>
						</TCEforms>
					</settings.offset>
					<settings.limit>
						<TCEforms>
							<label>LLL:EXT:vantomas/Resources/Private/Language/Site/LastUpdatedPages.xlf:flexform.settings.limit</label>
							<config>
								<type>input</type>
								<size>5</size>
								<eval>trim,int</eval>
								<range>
									<lower>1</lower>
									<upper>10</upper>
								</range>
								<default>1</default>
								<wizards>
									<settings.limit>
										<type>slider</type>
										<step>1</step>
										<width>100</width>
									</settings.limit>
								</wizards>
							</config>
						</TCEforms>
					</settings.limit>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
