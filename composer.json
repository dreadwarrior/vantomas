{
  "name": "dreadlabs/vantomas",
  "description": "A TYPO3.CMS project encapsulating the website www.van-tomas.",
  "type": "project",
  "keywords": [
    "typo3",
    "extension",
    "website",
    "project"
  ],
  "homepage": "http://www.van-tomas.de/",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Thomas Juhnke",
      "email": "typo3@van-tomas.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "DreadLabs\\Net\\": "src/DreadLabs/Net/",
      "DreadLabs\\VantomasWebsite\\": "src/DreadLabs/VantomasWebsite/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.van-tomas.de/"
    }
  ],
  "require": {
    "php": ">=5.4.4",
    "typo3-cms-extension/disqusapi": "*@dev",
    "typo3-cms-extension/rte4abstract": "*@dev",
    "net/http": "~1.1",
    "arg/tagcloud": "*@dev"
  },
  "require-dev": {
    "dreadlabs/typo3-build": "dev-master"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}