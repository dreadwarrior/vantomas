{
  "name": "dreadlabs/vantomas",
  "description": "A TYPO3.CMS project encapsulating the website www.van-tomas.",
  "type": "project",
  "keywords": [
    "typo3",
    "extension",
    "website",
    "project"
  ],
  "homepage": "http://www.van-tomas.de/",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Thomas Juhnke",
      "email": "typo3@van-tomas.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "DreadLabs\\Net\\": "src/DreadLabs/Net/",
      "DreadLabs\\VantomasWebsite\\": "src/DreadLabs/VantomasWebsite/src/",
      "DreadLabs\\Vantomas\\": "web/typo3conf/ext/vantomas/Classes/",
      "TYPO3\\CMS\\Core\\": "web/typo3_src/typo3/sysext/core/Classes",
      "TYPO3\\CMS\\Extbase\\": "web/typo3_src/typo3/sysext/extbase/Classes/"
    }
  },
  "require": {
    "php": ">=5.4.4",
    "net/http": "~1.1",
    "arg/tagcloud": "*@dev"
  },
  "require-dev": {
    "dreadlabs/typo3-build": "dev-master"
  },
  "suggest": {
    "apigen/apigen": "2.8.*"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}