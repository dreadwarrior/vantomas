      "TYPO3\\CMS\\CssStyledContent\\": "vendor/typo3/cms/typo3/sysext/css_styled_content/Classes/",
      "TYPO3\\CMS\\Dbal\\": "vendor/typo3/cms/typo3/sysext/dbal/Classes/",
