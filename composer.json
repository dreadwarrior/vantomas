      "TYPO3\\CMS\\Version\\": "vendor/typo3/cms/typo3/sysext/version/Classes/",
      "TYPO3\\CMS\\Viewpage\\": "vendor/typo3/cms/typo3/sysext/viewpage/Classes/",
