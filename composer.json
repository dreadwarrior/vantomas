      "TYPO3\\CMS\\Extbase\\": "vendor/typo3/cms/typo3/sysext/extbase/Classes/",
      "TYPO3\\CMS\\Extensionmanager\\": "vendor/typo3/cms/typo3/sysext/extensionmanager/Classes/",
