{
  "name": "dreadlabs/vantomas",
  "description": "A TYPO3.CMS project encapsulating the website www.van-tomas.",
  "type": "project",
  "keywords": [
    "typo3",
    "extension",
    "website",
    "project"
  ],
  "homepage": "http://www.van-tomas.de/",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Thomas Juhnke",
      "email": "typo3@van-tomas.de",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.typo3.org/"
    },
    {
      "type": "package",
      "package": {
        "name": "typo3-ter/beautyofcode",
        "version": "3.0-dev",
        "type": "typo3-cms-extension",
        "dist": {
          "url": "https://github.com/dreadwarrior/beautyofcode/archive/feature_54988_ci.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/dreadwarrior/beautyofcode.git",
          "type": "git",
          "reference": "feature_54988_ci"
        },
        "autoload": {
          "psr-4": {
            "TYPO3\\Beautyofcode\\": "Classes/"
          }
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "typo3-ter/realurl",
        "version": "1.13.4",
        "type": "typo3-cms-extension",
        "dist": {
          "url": "https://github.com/dmitryd/typo3-realurl/archive/1_13_4.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/dmitryd/typo3-realurl.git",
          "type": "git",
          "reference": "1_13_4"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "typo3/cms",
        "type": "typo3-cms-core",
        "version": "7.1.4",
        "dist": {
          "url": "https://github.com/TYPO3/TYPO3.CMS/archive/TYPO3_7-4-0.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/TYPO3/TYPO3.CMS.git",
          "type": "git",
          "reference": "TYPO3_7-4-0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5.9",

    "ext-fileinfo": "*",
    "ext-pcre": "*",
    "ext-session": "*",
    "ext-xml": "*",
    "psr/log": "1.0.*@dev",
    "pear/http_request2": "~2.2.1",
    "phpwhois/idna-convert": "~0.8.2",
    "swiftmailer/swiftmailer": "~5.4.1",
    "symfony/console": "~2.7.0",
    "symfony/finder": "~2.7.0",
    "doctrine/instantiator": "~1.0.4",
    "helhum/class-alias-loader": "^1.1.9",
    "typo3/cms-composer-installers": "^1.2.2",
    "psr/http-message": "~1.0",

    "typo3/cms": "~7.1",

    "typo3-ter/flux": "~7.2",
    "typo3-ter/fluidpages": "~3.3",
    "typo3-ter/fluidcontent": "~4.3",
    "typo3-ter/fluidcontent-core": "~1.2",
    "typo3-ter/vhs": "~2.3",
    "typo3-ter/realurl": "~1.13",
    "typo3-ter/beautyofcode": "3.0-dev",

    "arg/tagcloud": "*@dev",
    "dreadlabs/vantomas-website": "~5.0.0"
  },
  "require-dev": {
    "dreadlabs/typo3-build": "dev-master",
    "apigen/apigen": "~4.1"
  },
  "suggest": {
    "apigen/apigen": " Smart and Readable Documentation for your PHP project http://apigen.org"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\CMS\\About\\": "vendor/typo3/cms/typo3/sysext/about/Classes/",
      "TYPO3\\CMS\\Aboutmodules\\": "vendor/typo3/cms/typo3/sysext/aboutmodules/Classes/",
      "TYPO3\\CMS\\Backend\\": "vendor/typo3/cms/typo3/sysext/backend/Classes/",
      "TYPO3\\CMS\\Belog\\": "vendor/typo3/cms/typo3/sysext/belog/Classes/",
      "TYPO3\\CMS\\Beuser\\": "vendor/typo3/cms/typo3/sysext/beuser/Classes/",
      "TYPO3\\CMS\\ContextHelp\\": "vendor/typo3/cms/typo3/sysext/context_help/Classes/",
