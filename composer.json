      "TYPO3\\CMS\\Filelist\\": "vendor/typo3/cms/typo3/sysext/filelist/Classes/",
      "TYPO3\\CMS\\Fluid\\": "vendor/typo3/cms/typo3/sysext/fluid/Classes/",
      "TYPO3\\CMS\\Form\\": "vendor/typo3/cms/typo3/sysext/form/Classes/",
      "TYPO3\\CMS\\Frontend\\": "vendor/typo3/cms/typo3/sysext/frontend/Classes/",
      "TYPO3\\CMS\\Func\\": "vendor/typo3/cms/typo3/sysext/func/Classes/",
      "TYPO3\\CMS\\Impexp\\": "vendor/typo3/cms/typo3/sysext/impexp/Classes/",
      "TYPO3\\CMS\\IndexedSearch\\": "vendor/typo3/cms/typo3/sysext/indexed_search/Classes/",
      "TYPO3\\CMS\\IndexedSearchMysql\\": "vendor/typo3/cms/typo3/sysext/indexed_search_mysql/Classes/",
      "TYPO3\\CMS\\Info\\": "vendor/typo3/cms/typo3/sysext/info/Classes/",
      "TYPO3\\CMS\\InfoPagetsconfig\\": "vendor/typo3/cms/typo3/sysext/info_pagetsconfig/Classes/",
      "TYPO3\\CMS\\Install\\": "vendor/typo3/cms/typo3/sysext/install/Classes/",
      "TYPO3\\CMS\\Lang\\": "vendor/typo3/cms/typo3/sysext/lang/Classes/",
      "TYPO3\\CMS\\Linkvalidator\\": "vendor/typo3/cms/typo3/sysext/linkvalidator/Classes/",
