      "TYPO3\\CMS\\Reports\\": "vendor/typo3/cms/typo3/sysext/reports/Classes/",
      "TYPO3\\CMS\\Rsaauth\\": "vendor/typo3/cms/typo3/sysext/rsaauth/Classes/",
      "TYPO3\\CMS\\Rtehtmlarea\\": "vendor/typo3/cms/typo3/sysext/rtehtmlarea/Classes/",
      "TYPO3\\CMS\\Saltedpasswords\\": "vendor/typo3/cms/typo3/sysext/saltedpasswords/Classes/",
      "TYPO3\\CMS\\Scheduler\\": "vendor/typo3/cms/typo3/sysext/scheduler/Classes/",
      "TYPO3\\CMS\\Setup\\": "vendor/typo3/cms/typo3/sysext/setup/Classes/",
      "TYPO3\\CMS\\Sv\\": "vendor/typo3/cms/typo3/sysext/sv/Classes/",
      "TYPO3\\CMS\\SysAction\\": "vendor/typo3/cms/typo3/sysext/sys_action/Classes/",
      "TYPO3\\CMS\\SysNote\\": "vendor/typo3/cms/typo3/sysext/sys_note/Classes/",
      "TYPO3\\CMS\\T3editor\\": "vendor/typo3/cms/typo3/sysext/t3editor/Classes/",
      "TYPO3\\CMS\\T3skin\\": "vendor/typo3/cms/typo3/sysext/t3skin/Classes/",
      "TYPO3\\CMS\\Taskcenter\\": "vendor/typo3/cms/typo3/sysext/taskcenter/Classes/",
