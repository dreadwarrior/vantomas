{
	"name": "van-tomas.de website",
	"type": "typo3-cms-extension",
	"description": "The website www.van-tomas.de",
	"authors": [{
		"name": "Thomas Juhnke",
		"email": "tommy@van-tomas.de"
	}],
	"autoload": {
		"classmap": ["src/", "vendor/typo3/cms"]
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.van-tomas.de/"
		}
	],
	"require": {
		"php": ">=5.4.4",
		"typo3/cms-composer-installers": "1.1.*-dev",
		"typo3/cms": "6.2.*-dev",
		"typo3-cms-extension/beautyofcode": "dev-master",
		"typo3-cms-extension/disqusapi": "dev-master",
		"typo3-cms-extension/fluidcontent": "4.0.0",
		"typo3-cms-extension/fluidcontent_core": "dev-master",
		"typo3-cms-extension/fluidpages": "3.0.0",
		"typo3-cms-extension/flux": "7.0.0",
		"typo3-cms-extension/gridelements": "2.0.*",
		"typo3-cms-extension/static_info_tables": "6.0.*",
		"typo3-cms-extension/static_info_tables_de": "6.0.*",
		"typo3-cms-extension/vge_tagcloud": "1.7.0",
		"typo3-cms-extension/vhs": "1.8.5",
		"net/http": "1.0.*"
	},
	"require-dev": {
		"phing/phing": "2.5.*",
		"phpunit/phpunit": "3.*",
		"phpmd/phpmd": "1.4.*",
		"pdepend/pdepend": "1.1.*",
		"phploc/phploc": "*",
		"squizlabs/php_codesniffer": "1.*",
		"sebastian/phpcpd": "*"
	}
}